{% comment %}
  {{ 'customer.css' | asset_url | stylesheet_tag }}

  {%- style -%}
    .section-{{ section.id }}-padding {
      padding-top: {{ section.settings.padding_top | times: 0.75 | round: 0 }}px;
      padding-bottom: {{ section.settings.padding_bottom | times: 0.75 | round: 0 }}px;
    }

    @media screen and (min-width: 750px) {
      .section-{{ section.id }}-padding {
        padding-top: {{ section.settings.padding_top }}px;
        padding-bottom: {{ section.settings.padding_bottom }}px;
      }
    }
  {%- endstyle -%}
{% endcomment %}

<style type="text/css">
  .aaa_formbuilder_container {
    padding: 10px;
  }

  .aaaformbuilder_form .form-group {
    width: 100%;
    display: inline-flex;
    flex-direction: column;
    margin-bottom: 10px;
  }

  .aaaformbuilder_form .fb-submit button {
    background-color: #00afec !important;
  }
  .aaaformbuilder_form .fb-submit button {
    color: #ffffff !important;
  }
  .aaaformbuilder_form .fb-submit button {
    font-size: 14px !important;
  }
  .aaaformbuilder_form input {
    height: 4.5rem;
  }
  .aaa_formbuilder_container h1.header {
    display: block;
    text-align: center;
    margin-bottom: 2rem;
    font-weight: 600;
  }

  .aaa_formbuilder_container h2.header {
    display: block;
    font-weight: 600;
  }

  .aaaformbuilder_form .form-group select {
    border: 1px solid rgb(118, 118, 118);
  }

  .aaaformbuilder_form .form-group button {
    max-width: 350px;
    margin: 0 auto;
  }

  @media only screen and (max-width: 768px) {
    .customer.register .aaa_formbuilder_container {
      width: 100% !important;
    }
  }

  @media screen and (min-width: 769px) {
    .aaaformbuilder_form .form-group:has(> .half-width) {
      width: calc(50% - 20px);
      margin-right: 10px;
    }
  }
</style>

